    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.4.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
