{
  "name": "@pxp-ui/core",
  "version": "1.1.2",
  "description": "core for pxp-ui",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prepublish": "tsc",
  "scripts": {
    "build": "rimraf ./dist && tsc && npm run build:copy-files",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "dev:publish": "cd ./dist && yalc publish",
    "prod:publish": "cd ./dist && npm publish",
    "start": "npm run build && node dist/index.js",
    "dev": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "pxp-ui",
    "pxp"
  ],
  "author": "Kplian",
  "license": "ISC",
